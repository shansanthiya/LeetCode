bool hasCycle(ListNode *head) {
        
        if(head == NULL)
            return false;
        
        ListNode* fastptr = head;
        ListNode* slowptr = head;
        
        while(fastptr && slowptr && fastptr->next)
        {
            fastptr = fastptr->next->next;
            slowptr = slowptr->next;
            if(fastptr == slowptr)
            return true;
        }
            return false;
    }
