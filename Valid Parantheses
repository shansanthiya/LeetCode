bool isValid(string s) {
        stack<char> st;
        int len = s.length();
        for(int i =0 ; i < len; i++)
        {
            switch(s[i])
            {
                case '(':
                case '{':
                case '[': st.push(s[i]);
                            break;
                case ')': if(st.empty() || st.top() != '(')
                                return false;
                            else 
                                st.pop();
                            break;
                case '}':if(st.empty() || st.top() != '{')
                                return false;
                            else 
                                st.pop();
                            break;
                case ']':if(st.empty() || st.top() != '[')
                                return false;
                            else 
                                st.pop();
                            break;
            }
                
        }
        
        return st.empty();
    }
};
